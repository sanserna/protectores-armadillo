.row {
  max-width 75rem
  margin-left auto
  margin-right auto
  display flex
  flex-flow row wrap
  .column.row.row, .row.row.columns {
    padding-left 0
    padding-right 0
    margin-left 0
    margin-right 0
  }
  .row {
    margin-left -0.625rem
    margin-right -0.625rem
    @media screen and (min-width: 40em) {
      margin-left -0.9375rem
      margin-right -0.9375rem
    }
  }
}


.row.expanded {
  max-width none
}

.row.collapse {
  > .column, > .columns {
    padding-left 0
    padding-right 0
